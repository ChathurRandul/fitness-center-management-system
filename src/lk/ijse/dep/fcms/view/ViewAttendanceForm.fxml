<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep.fcms.controller.ViewAttendanceFormController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" layoutX="2.0" layoutY="2.0" opacity="0.1" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../asset/background.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tblAttendance" layoutX="52.0" layoutY="78.0" prefHeight="500.0" prefWidth="700.0">
         <columns>
            <TableColumn fx:id="colDate" prefWidth="124.0" text="Date" />
            <TableColumn fx:id="colId" prefWidth="111.0" text="Member ID" />
            <TableColumn fx:id="colName" prefWidth="257.0" text="Member Name" />
            <TableColumn fx:id="colInTime" prefWidth="100.0" text="In Time" />
            <TableColumn fx:id="colOutTime" prefWidth="107.0" text="Out Time" />
         </columns>
      </TableView>
      <ImageView fx:id="btnBack" fitHeight="35.0" fitWidth="28.0" layoutX="23.0" layoutY="22.0" onMouseClicked="#btnBack_OnMouseClicked" onMouseEntered="#btnBack_OnMouseEntered" onMouseExited="#btnBack_OnMouseExited" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../asset/back.png" />
         </image>
      </ImageView>
      <JFXTextField fx:id="txtSearch" focusColor="BLACK" layoutX="252.0" layoutY="24.0" prefHeight="24.0" prefWidth="300.0" promptText="Search">
         <font>
            <Font name="Cantarell Regular" size="18.0" />
         </font>
      </JFXTextField>
   </children>
</AnchorPane>
